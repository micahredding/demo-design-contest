<p id="notice"><%= notice %></p>

<h1>Submissions</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Description</th>
      <th>Published</th>
      <th>Contest</th>
      <th>Rating</th>
      <th>Status</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @submissions.each do |submission| %>
      <tr>
        <td><%= submission.title %></td>
        <td><%= submission.description %></td>
        <td><%= submission.published %></td>
        <td><%= submission.contest %></td>
        <td><%= submission.rating %></td>
        <td><%= submission.status %></td>
        <td><%= link_to 'Show', contest_submission_path(@contest, submission) %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Submission', new_contest_submission_path %>
